{
  "name": "root",
  "private": true,
  "scripts": {
    "start": "start-storybook",
    "build": "build-storybook -c .storybook -o docs",
    "prebuild": "rm -rf docs; mkdir docs;",
    "publish-latest": "lerna publish --force-publish",
    "publish-beta": "lerna publish --force-publish --dist-tag beta",
    "build:weapp": "taro build --type weapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "svg": "rm -rf packages/components/src/fonts; node svg2font.ts;",
    "link": "cd packages/c-react; yarn link; cd ../mp; yarn link; cd ../mp-request; yarn link; cd ../c-tool;  yarn link; cd ../c-font; yarn link; cd ../c-qrcode; yarn link; cd ../mp-business; yarn link; cd ../c-cookie; yarn link; cd ../c-service-time; yarn link;"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "./packages/**/*.js": [
      "eslint --cache --fix"
    ],
    "./packages/**/*.ts": [
      "eslint --cache --fix"
    ],
    "./packages/**/*.less": [
      "stylelint --cache --fix"
    ]
  },
  "browserslist": {
    "production": [
      "iOS >= 8",
      "Android >= 5.0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "commitlint": {
    "extends": [
      "@gm-react-app/commitlint-gm-react-app"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
