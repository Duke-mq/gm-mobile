{
  "extends": "ts-config-gm-react-app/tsconfig",
  "types" : [
    "react-dom",
    "swiper",
  ],
  "compilerOptions": {
    "jsx": "react",
    "importsNotUsedAsValues": "preserve",
    "esModuleInterop": true,
    "paths": {
      "@/*": ["mini-program/*"]
    }
  },
  "include": [
    "**/*.ts", "**/*.tsx",
    "packages/**/*",
    "global.d.ts",
  ],
  "exclude": ["node_modules"]
}
